{
  "root": true,
  "extends": ["./node_modules/sanctuary-style/eslint-es6.json"],
  "overrides": [
    {
      "files": ["*.md"],
      "globals": {"$": "readonly", "Constant": "readonly", "S": "readonly", "Z": "readonly"}
    },
    {
      "files": ["index.js"],
      "globals": {"Deno": "readonly"},
      "rules": {
        "no-unused-vars": ["error", {"args": "none", "varsIgnorePattern": "^S$"}]
      }
    }
  ]
}
