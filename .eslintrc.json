{
  "root": true,
  "extends": ["./node_modules/sanctuary-style/eslint-es6.json"],
  "overrides": [
    {
      "files": ["*.md"],
      "globals": {"$": false, "Constant": false, "S": false, "Z": false}
    },
    {
      "files": ["index.js"],
      "globals": {"Deno": false},
      "rules": {
        "no-unused-vars": ["error", {"args": "none", "varsIgnorePattern": "^S$"}]
      }
    }
  ]
}
